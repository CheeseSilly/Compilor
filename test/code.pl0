{The Man Who Sold the }World}
const z = 0;
var head(1:10), foot, cock, rabbit, n;
begin
    n := z;
    read(head(z), foot)
;
    cock := 1;
    while cock <= head do
    begin
        rabbit := head - cock;
        if cock * 2 + rabbit * 4 = foot then
        begin
            write(cock, rabbit);
            n := n + 1;
        end
        cock := cock + 1;
    end;
    if n = 0 then write(0,0);else write(1,0);
    
end.